<h1>New PlantTag</h1>

<div class="container">
  <div class="row justify-content-center">
    <div class="col-8">
      <%= simple_form_for [@plant, @plant_tag] do |f| %>
      <%= f.association :tag %>
      <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
